        "eslint-plugin-prettier": "^5.4.1",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.0",
        "prettier": "^3.5.3"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
